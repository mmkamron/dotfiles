(defcfg
  process-unmapped-keys yes
)

(defsrc
  tab            bspc f12
  caps h j k l
  lsft lalt lmet spc rmet
)

(deflayer default
  @nav              del prnt
  @cap _ _ _ _
  _ _ _ _ bspc
)

(deflayer arrows
  _ _ _
  _ left down up rght
  _ _ _ _ _
)

(defalias
  cap (tap-hold-press 200 200 caps lctl)
  nav (tap-hold-press 200 200 tab (layer-toggle arrows))
)
